{% extends 'base.html.twig' %}

{% block title %}Hello UserController!{% endblock %}

{% block body %}

{{form_start(form)}}

<div class="row container">
  <div class="col-md-6">
    {{form_row(form.nom)}}
    {{form_row(form.prenom)}}
    {{form_row(form.password,{'attr':{ disabled:'disabled'}})}}
  </div>
  <div class="col-md-6">
    {{form_row(form.email)}}
    {{form_row(form.datedeNaissance)}}
    {{form_row(form.submit)}}
    
  </div>
</div>

{{form_end(form)}}
{% endblock %}
